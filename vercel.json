{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "*.{html,js,css}",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(health|predict|docs|openapi.json)",
            "dest": "/main.py"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
